<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<fx:root type="FlowPane" xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         prefHeight="400.0" prefWidth="600.0">
    <VBox maxWidth="780" maxHeight="580">
        <FlowPane maxWidth="780" maxHeight="580">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
            <ImageView fx:id="imageView" fitWidth="780" fitHeight="580" preserveRatio="true">
                <viewport>
                    <Rectangle2D width="760" height="460">

                    </Rectangle2D>
                </viewport>
            </ImageView>
        </FlowPane>
        <Label fx:id="selectedItemLabel">

        </Label>
    </VBox>
</fx:root>