<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<fx:root type="MenuBar" xmlns:fx="http://javafx.com/fxml/1">
    <Menu text="File">
        <MenuItem text="Scan Folder" onAction="#performScan"/>
        <MenuItem text="Save Image"/>
        <MenuItem text="Test JPG Tag" onAction="#testJPGTag"/>
        <SeparatorMenuItem/>
        <MenuItem text="Exit" onAction="#exitAction"/>
    </Menu>
    <Menu text="Edit">
        <MenuItem text="Rotate"/>
        <MenuItem text="Crop"/>
    </Menu>
    <Menu text="Help">
        <MenuItem text="Online Manual" visible="false"/>
        <SeparatorMenuItem/>
        <MenuItem text="About"/>
    </Menu>
</fx:root>